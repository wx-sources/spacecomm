	promptOverlay.Size = UDim2.new(1, 0, 1, 36)

	ErrorPrompt.Name = "ErrorPrompt"
	ErrorPrompt.Parent = promptOverlay
	ErrorPrompt.AnchorPoint = Vector2.new(0.5, 0.5)
	ErrorPrompt.BackgroundColor3 = Color3.fromRGB(57, 59, 61)
