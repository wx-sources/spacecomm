	ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling  

	promptOverlay.Name = "promptOverlay"
	promptOverlay.Parent = ScreenGui
	promptOverlay.Active = true
	promptOverlay.BackgroundTransparency = 1
