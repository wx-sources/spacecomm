	TitleFramePadding.PaddingTop = UDim.new(0, 11)

	ErrorTitle.Parent = TitleFrame
	ErrorTitle.BackgroundTransparency = 1
	ErrorTitle.Size = UDim2.new(1, 0, 0, 28)
	ErrorTitle.ZIndex = 8
